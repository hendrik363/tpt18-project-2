<Window x:Class="Snake_Hendrik_Metsallik.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Snake_Hendrik_Metsallik"
        mc:Ignorable="d"
        Title="Snake Game" Height="450" Width="800" MouseDown="Window_MouseDown" KeyDown="Window_KeyDown">
    <Grid>
        <Canvas Background="LightGreen">
            <Rectangle Height="80" Width="200" 
                   Stroke="Black" Fill="Blue" 
                   StrokeThickness="4"
                   Canvas.Left="20" Canvas.Top="20"
                   Name="rectangle1">
            </Rectangle>
        </Canvas>
    </Grid>
</Window>
